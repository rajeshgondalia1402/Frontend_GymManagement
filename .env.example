# ============================================
# GymPro Frontend — Environment Variables Template
# ============================================
# Copy this file to .env.development or .env.production and fill in values
#
# Vite auto-loads:
#   .env.development  → when running `npm run dev`
#   .env.production   → when running `npm run build`
#
# All env vars MUST start with VITE_ to be exposed to the frontend.

VITE_APP_ENV=development
VITE_BACKEND_URL=http://localhost:5000
VITE_API_URL=http://localhost:5000/api/v1
VITE_PORT=3005
VITE_ENABLE_DEBUG=true
VITE_APP_TITLE=GymPro
